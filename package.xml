      <role>lead</role>
    </maintainer>
  </maintainers>

  <release>
   <version>0.1</version>
   <state>beta</state>
   <date>2006-01-12</date>
   <notes>Initial Release</notes>
  </release>

  <filelist>
   <file role="doc" name="CREDITS"/>
   <file role="doc" name="EXPERIMENTAL"/>
   <file role="src" name="config.m4"/>
   <file role="src" name="config.w32"/>
   <file role="src" name="operator.c"/>
